<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cheayoung.androidkotlin">
    <!-- 인터넷 permission 권한 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".FragmentActivity"></activity>
        <activity android:name=".RecyclerActivity" />
        <activity android:name=".WebSiteActivity" />
        <activity android:name=".DBActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".CustomListviewActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".SubActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>